    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@types/lodash": "^4.17.9",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
